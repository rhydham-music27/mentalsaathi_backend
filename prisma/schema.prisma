// prisma/schema.prisma
generator client {
  provider = "prisma-client-js"
}


datasource db {
  provider  = "postgresql"
  url  	    = env("DATABASE_URL")
  // uncomment next line if you use Prisma <5.10
  directUrl = env("DATABASE_URL_UNPOOLED")
}
model WaitList{
  id Int @unique @default(autoincrement())
  email String @unique
}

model GetInTouch{
  id  Int @unique @default(autoincrement())
  name String 
  phone Int @unique
  role String
  institution String

}
